{% block top_of_page %}{% endblock %}
<!DOCTYPE html>
<!--[if lt IE 9]><html class="lte-ie8" lang="{{ html_lang|default('en') }}"><![endif]-->
<!--[if gt IE 8]><!--><html lang="{{ html_lang|default('en') }}"><!--<![endif]-->
  <head>
    <meta charset="utf-8" />
    <title>{% block page_title %}GOV.UK - The best place to find government services and information{% endblock %}</title>

    <!--[if gt IE 8]><!--><link rel="stylesheet" media="screen" href="/css/govuk-template.css"/><!--<![endif]-->
    <!--[if IE 6]><link rel="stylesheet" media="screen" href="/css/govuk-template-ie6.css?0.23.0"/><![endif]-->
    <!--[if IE 7]><link rel="stylesheet" media="screen" href="/css/govuk-template-ie7.css?0.23.0"/><![endif]-->
    <!--[if IE 8]><link rel="stylesheet" media="screen" href="/css/govuk-template-ie8.css?0.23.0"/><![endif]-->
    <link rel="stylesheet" media="print" href="/css/govuk-template-print.css?0.23.0"/>

    <!--[if IE 8]><link rel="stylesheet" media="all" href="/css/fonts-ie8.css?0.23.0"/><![endif]-->
    <!--[if gte IE 9]><!--><link rel="stylesheet" media="all" href="/css/fonts.css?0.23.0"/><!--<![endif]-->
    <!--[if lt IE 9]><script src="/scripts/ie.js?0.23.0"></script><![endif]-->
    <!--[if lte IE 9]><script src="/scripts/ie9.js"></script><![endif]-->

    <link rel="shortcut icon" href="/images/favicon.ico?0.23.0" type="image/x-icon" />

    <link rel="mask-icon" href="/images/gov.uk_logotype_crown.svg?0.23.0" color="#0b0c0c">
    <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon-180x180.png?0.23.0">
    <link rel="apple-touch-icon" sizes="167x167" href="/images/apple-touch-icon-167x167.png?0.23.0">
    <link rel="apple-touch-icon" sizes="152x152" href="/images/apple-touch-icon-152x152.png?0.23.0">
    <link rel="apple-touch-icon" href="/images/apple-touch-icon.png?0.23.0">

    <!--[if gt IE 8]><!--><!--<![endif]-->



    <meta name="theme-color" content="#0b0c0c" />

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:image" content="/images/opengraph-image.png?0.23.0">
    <link rel="stylesheet" media="screen" href="/css/main.css"/>
    {% block head %}{% endblock %}
  </head>

  <body class="{% block body_classes %}{% endblock %}">
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

    {% block body_start %}{% endblock %}

    <div id="skiplink-container">
      <div>
        <a href="#content" class="skiplink">{{ skip_link_message|default('Skip to main content') }}</a>
      </div>
    </div>

    <div id="global-cookie-message" class="cookie-message">
        {% block cookie_message %}Congratulations! You've collected the csrf cookie. Keep collecting!{% endblock %}
    </div>

    <header role="banner" id="global-header" class="{% block header_class %}{% endblock %}">
      <div class="header-wrapper">
        <div class="header-global">
          <div class="header-logo">
            <a href="https://dwpdigital.blog.gov.uk/" title="DWP Digital blog" class="content main-logo digital-link">
              {# <img src="/images/gov.uk_logotype_crown_invert_trans.png?0.23.0" width="36" height="32" alt=""> {{ global_header_text|default('GOV.UK') }} #}
              DWP Digital
            </a>
          </div>
          {% block inside_header %}{% endblock %}
          {% if userToken and not tokenRaw %}
          <form class="form right header-logout-form" action="/logout" method="post">
              <input type="hidden" name="_csrf" value="{{ _csrf }}">
              <input class="header-input" type="submit" name="logout" value="Logout">
          </form>
          <div class="clearfix"></div></div>
        </div>
        {% endif %}
        {% block proposition_header %}{% endblock %}
    </header>


    {% block after_header %}{% endblock %}

    <div id="global-header-bar"></div>

    <main class="main content" id="content" role="main">
        <div class="grid-row">
            <div class="column-two-thirds">
                {% block pre_content %}{% endblock %}
            </div>
        </div>

        <div class="grid-row">

            {% block content %}{% endblock %}
        </div>

    </main>

    <footer class="group js-footer" id="footer" role="contentinfo">

      <div class="footer-wrapper">
        {% block footer_top %}{% endblock %}

        <div class="footer-meta">
          <div class="footer-meta-inner">
            {% block footer_support_links %}{% endblock %}
            <ul>
                <li>Made by Burbank for the <a href="/">Department for Work and Pensions</a></li>
                <li style="display: block;">Book page designed in Newcastle</li>
            </ul>
            <div class="open-government-licence">

              <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>

                {% block licence_message %}<p>All content is available under the <a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence v3.0</a>, except where otherwise stated</p>{% endblock %}

            </div>
          </div>

          <div class="copyright">
            <a href="https://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">{{ crown_copyright_message|default('&copy; Crown copyright')|safe }}</a>
          </div>
        </div>
      </div>
    </footer>

    <div id="global-app-error" class="app-error hidden"></div>

    <script src="/scripts/govuk-template.js"></script>
    <script src="/scripts/main.js"></script>


    {% block body_end %}{% endblock %}


    <script>if (typeof window.GOVUK === 'undefined') document.body.className = document.body.className.replace('js-enabled', '');</script>
  </body>
</html>
